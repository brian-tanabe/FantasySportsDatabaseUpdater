<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <import resource="classpath:/spring-configuration/webpages-configuration.xml"/>

    <bean id="espnProjectionsNextPageValueExtractor" class="com.btanabe.fsdu.parsers.ValueExtractor">
        <constructor-arg name="inputHtmlString" ref="espnProjectionsPageOneFactoryBean"/>
        <constructor-arg name="outputClasspath" value="java.lang.String"/>
        <constructor-arg name="outputMatchingRegex"
                         value="(?&lt;=href=&quot;).+startIndex.+(?=&quot;&gt;NEXT)"/>
    </bean>

    <bean id="espnProjectionsRushingYardsProjection" class="com.btanabe.fsdu.parsers.ValueExtractor">
        <constructor-arg name="inputHtmlString" ref="espnProjectionsPageEddieLacyBean"/>
        <constructor-arg name="outputClasspath" value="java.lang.Integer"/>
        <constructor-arg name="outputMatchingRegex"
                         value="(?&lt;=&lt;td class=&quot;playertableStat&quot;&gt;\d{1,5}&lt;\/td&gt;\n&lt;td class=&quot;playertableStat&quot;&gt;\d{1,5}&lt;\/td&gt;\n&lt;td class=&quot;playertableStat&quot;&gt;\d{1,5}&lt;\/td&gt;\n&lt;td class=&quot;playertableStat&quot;&gt;\d{1,5}&lt;\/td&gt;\n&lt;td class=&quot;playertableStat&quot;&gt;)\d+(?=&lt;/td&gt;)"/>
    </bean>

    <bean id="espnProjectionsFantasyPointsProjection" class="com.btanabe.fsdu.parsers.ValueExtractor">
        <constructor-arg name="inputHtmlString" ref="espnProjectionsPageEddieLacyBean"/>
        <constructor-arg name="outputClasspath" value="java.lang.Float"/>
        <constructor-arg name="outputMatchingRegex"
                         value="(?&lt;=&lt;td class=&quot;playertableStat appliedPoints&quot;&gt;)\d{1,4}\.\d{1,2}(?=&lt;/td&gt;)"/>
    </bean>

    <bean id="espnProjectionsNameValueExtractor" class="com.btanabe.fsdu.parsers.ValueExtractor">
        <constructor-arg name="outputClasspath" value="java.lang.String" />
        <constructor-arg name="outputMatchingRegex" value="" />
    </bean>
</beans>