<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <import resource="classpath:/spring-configuration/test-webpages-configuration.xml"/>
    <import resource="classpath:/spring-configuration/value-extractors-configuration.xml"/>
    <import resource="classpath:/spring-configuration/model-factory-configuration.xml"/>
    <import resource="classpath:/spring-configuration/application-configuration.xml" />
    <import resource="classpath:/spring-configuration/record-parser-configuration.xml" />

    <bean id="eddieLacyEspnPlayerProjectionRecordParser" class="com.btanabe.fsdu.parsers.RecordParser">
        <constructor-arg ref="espnPlayerProjectionRecordParserValueExtractorMap"/>
        <constructor-arg ref="&amp;espnNflProjectionModelFactory"/>
        <property name="inputHtml" ref="espnProjectionsPageEddieLacy"/>
    </bean>

    <bean id="aaronRodgersEspnPlayerProjectionRecordParser" class="com.btanabe.fsdu.parsers.RecordParser">
        <constructor-arg ref="espnPlayerProjectionRecordParserValueExtractorMap"/>
        <constructor-arg ref="&amp;espnNflProjectionModelFactory"/>
        <property name="inputHtml" ref="espnProjectionsPageAaronRodgers"/>
    </bean>

    <bean id="testEspnPlayerProjectionPagePaginatedRecordParser" class="com.btanabe.fsdu.parsers.PaginatedRecordParser">
        <constructor-arg ref="espnProjectionsPagePlayerValueExtractor"/>
        <constructor-arg ref="espnPlayerProjectionRecordParserValueExtractorMap"/>
        <constructor-arg ref="&amp;espnNflProjectionModelFactory"/>
        <property name="inputHtml" ref="espnNflProjectionsPageOneFormatted"/>
    </bean>
</beans>
