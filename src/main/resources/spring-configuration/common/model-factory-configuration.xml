<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <import resource="classpath:/spring-configuration/common/class-types.xml"/>

    <bean id="espnNflProjectionModelFactory" class="com.btanabe.fsdu.models.factories.ModelFactory">
        <property name="outputClazz" ref="espnNflProjectionModel"/>
    </bean>

    <bean id="espnNbaProjectionModelFactory" class="com.btanabe.fsdu.models.factories.ModelFactory">
        <property name="outputClazz" ref="espnNbaProjectionModel"/>
    </bean>

    <bean id="espnNbaPlayerRaterPagePlayerOwnershipModelFactory" class="com.btanabe.fsdu.models.factories.ModelFactory">
        <property name="outputClazz" ref="espnNbaPlayerRaterPagePlayerOwnershipModel"/>
    </bean>

    <bean id="basketballReferenceSeasonTotalsModelFactory" class="com.btanabe.fsdu.models.factories.ModelFactory">
        <property name="outputClazz" ref="basketballReferenceSeasonTotalsModel"/>
    </bean>

    <bean id="numberFireYearlyProjectionsModelFactory" class="com.btanabe.fsdu.models.factories.ModelFactory">
        <property name="outputClazz" ref="numberFirePlayerIdModel"/>
    </bean>
</beans>