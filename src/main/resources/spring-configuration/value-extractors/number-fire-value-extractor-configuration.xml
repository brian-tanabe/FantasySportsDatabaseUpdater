<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="numberFireYearlyProjectionsPagePlayerValueExtractor" class="com.btanabe.fsdu.parsers.JsonValueExtractor">
        <constructor-arg name="outputClasspath" value="java.lang.String"/>
        <constructor-arg name="jsonObjectMatchingRegex" value="(?&lt;=&quot;players&quot;:)(.*)(?=,&quot;related_articles&quot;:)"/>
    </bean>


    <bean id="numberFireFirstNameValueExtractor" class="com.btanabe.fsdu.parsers.JsonValueExtractor">
        <constructor-arg name="outputClasspath" value="java.lang.String"/>
        <constructor-arg name="jsonObjectMatchingRegex" value="(?&lt;=&quot;players&quot;:)(.*)(?=,&quot;related_articles&quot;:)"/>

    </bean>
</beans>