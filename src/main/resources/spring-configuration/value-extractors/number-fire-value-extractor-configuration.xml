<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="numberFireYearlyProjectionsPagePlayerValueExtractor" class="com.btanabe.fsdu.parsers.PaginatedValueExtractor">
        <constructor-arg name="outputClasspath" value="java.lang.String"/>
        <constructor-arg name="outputMatchingRegex" value="&quot;\d{0,4}&quot;:\{&quot;id&quot;:&quot;\d{0,4}&quot;,&quot;name&quot;:&quot;[A-Za-z ]{1,50}&quot;,&quot;slug&quot;:&quot;[a-zA-Z\-]{1,25}&quot;,&quot;first_name&quot;:&quot;[A-Za-z\. ]{1,25}&quot;,&quot;last_name&quot;:&quot;[A-Za-z\. -]{1,25}&quot;,&quot;race&quot;:&quot;\d&quot;,&quot;position&quot;:&quot;[A-Z]{1,3}&quot;,&quot;position_group&quot;:&quot;\d&quot;,&quot;team_id&quot;:&quot;\d{0,2}&quot;,&quot;number&quot;:&quot;\d{0,2}&quot;,&quot;height&quot;:&quot;\d{0,2}&quot;,&quot;weight&quot;:&quot;\d{0,3}&quot;,&quot;age&quot;:&quot;\d{0,2}&quot;,&quot;experience&quot;:&quot;\d{0,2}&quot;,&quot;dob&quot;:&quot;\d{4}-\d{2}-\d{2}&quot;,&quot;url&quot;:&quot;&quot;,&quot;yahoo_id&quot;:&quot;\d{0,6}&quot;,&quot;espn_id&quot;:&quot;\d{0,8}&quot;,&quot;league_id&quot;:&quot;\d{0,6}&quot;,&quot;sports_reference_id&quot;:&quot;[a-zA-Z0-9]{1,20}&quot;,&quot;role_id&quot;:&quot;\d&quot;,&quot;category_id&quot;:&quot;\d&quot;,&quot;college&quot;:&quot;&quot;,&quot;is_active&quot;:&quot;\d&quot;,&quot;is_rookie&quot;:&quot;\d&quot;,&quot;depth_position&quot;:&quot;(PG|SG|SF|PF|C){1,5}&quot;,&quot;depth_rank&quot;:&quot;\d&quot;\}"/>
    </bean>

    <bean id="numberFireFullNameValueExtractor" class="com.btanabe.fsdu.parsers.ValueExtractor">
        <constructor-arg name="outputClasspath" value="java.lang.String"/>
        <constructor-arg name="outputMatchingRegex" value="(?&lt;=&quot;name&quot;:&quot;)(.+?)(?=&quot;,)"/>
    </bean>

    <bean id="numberFireFirstNameValueExtractor" class="com.btanabe.fsdu.parsers.ValueExtractor">
        <constructor-arg name="outputClasspath" value="java.lang.String"/>
        <constructor-arg name="outputMatchingRegex" value="(?&lt;=&quot;first_name&quot;:&quot;)(.+?)(?=&quot;,)"/>
    </bean>

    <bean id="numberFireLastNameValueExtractor" class="com.btanabe.fsdu.parsers.ValueExtractor">
        <constructor-arg name="outputClasspath" value="java.lang.String"/>
        <constructor-arg name="outputMatchingRegex" value="(?&lt;=&quot;last_name&quot;:&quot;)(.+?)(?=&quot;,)"/>
    </bean>

    <bean id="numberFireEspnIdValueExtractor" class="com.btanabe.fsdu.parsers.ValueExtractor">
        <constructor-arg name="outputClasspath" value="java.lang.Integer"/>
        <constructor-arg name="outputMatchingRegex" value="(?&lt;=&quot;espn_id&quot;:&quot;)(.+?)(?=&quot;,)"/>
    </bean>

    <bean id="numberFireYahooIdValueExtractor" class="com.btanabe.fsdu.parsers.ValueExtractor">
        <constructor-arg name="outputClasspath" value="java.lang.Integer"/>
        <constructor-arg name="outputMatchingRegex" value="(?&lt;=&quot;yahoo_id&quot;:&quot;)(.+?)(?=&quot;,)"/>
    </bean>

    <bean id="numberFireBasketballReferenceIdValueExtractor" class="com.btanabe.fsdu.parsers.ValueExtractor">
        <constructor-arg name="outputClasspath" value="java.lang.String"/>
        <constructor-arg name="outputMatchingRegex" value="(?&lt;=&quot;sports_reference_id&quot;:&quot;)(.+?)(?=&quot;,)"/>
    </bean>
</beans>