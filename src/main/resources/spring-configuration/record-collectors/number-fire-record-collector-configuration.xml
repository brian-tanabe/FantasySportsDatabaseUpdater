<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <import resource="classpath:/spring-configuration/value-extractors/number-fire-value-extractor-configuration.xml"/>
    <import resource="classpath:/spring-configuration/record-parsers/number-fire-record-parser-configuration.xml"/>

    <bean id="numberFireYearlyProjectionsPageRecordCollector" class="com.btanabe.fsdu.collectors.RecordCollector">
        <constructor-arg ref="numberFireNextPageValueExtractor"/>
        <constructor-arg ref="numberFireYearlyProjectionsPagePaginatedRecordParser"/>
        <constructor-arg ref="hashSetClass"/>
    </bean>

    <bean id="hashSetClass" class="java.lang.Class" factory-method="forName">
        <constructor-arg value="java.util.HashSet"/>
    </bean>
</beans>